h-react-hooks/src/reportWebVitals.ts",[],[],[],"/Users/darwish/Desktop/darwish-react-hooks/src/App.tsx",["125","126"],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useReactState/UseReactState.demo.tsx",[],[],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useReactState/index.ts",["127"],[],"import React from \"react\";\nimport useDisplayDevError from \"../useDisplayDevError\";\nconst useReactState = <T extends object>(initialValue: T) => {\n  const [state, setState] = React.useState(initialValue);\n  const dispatchError = useDisplayDevError();\n\n  const proxy = React.useMemo(() => {\n    return new Proxy(initialValue, {\n      set(obj, prop, value) {\n        try {\n          obj[prop as keyof T] = value;\n          setState((prevState) => {\n            return {\n              ...prevState,\n              prop: state\n            }\n          });\n          return true\n        } catch(error) {\n          dispatchError(error);\n          return false;\n        }\n\n      }\n    })\n  }, []);\n\n  return proxy;\n}\n\nexport default useReactState;","/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useDisplayDevError/index.ts",["128","129"],[],"import * as React from 'react';\n\n/**\n * \n * @param errorText 默认错误信息\n * @returns 返回出发错误信息的函数，⚠️注意：错误信息只会在开发环境触发\n */\nconst useDisplayDevError: <T>(errorText?: T) => (err?: T) => void = (errorText) => {\n  const [error, setError] = React.useState(errorText);\n\n  React.useEffect(() => {\n    if (process.env.NODE_ENV === 'development' && error) {\n      console.error(errorText);\n    }\n  }, [error])\n\n  const dispatchError = React.useCallback((err?: any) => {\n    return setError(typeof err === 'undefined' ? errorText : err)\n  }, [])\n\n  return dispatchError\n}\n\nexport default useDisplayDevError","/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useTimeout/UseTimeout.demo.tsx",["130"],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useTimeoutFn/index.tsx",["131"],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useTimeoutFn/index.demo.tsx",["132"],[],"/Users/darwish/Desktop/darwish-react-hooks/src/functions/deepEqual.ts",[],[],"/Users/darwish/Desktop/darwish-react-hooks/src/functions/index.ts",[],[],[],"/Users/darwish/Desktop/darwish-react-hooks/src/functions/isBlanks.ts",["133"],[],"/**\n *\n * @param data 支持所有基本类型,还有[],{},Set,Map,Date的判断，true则数据空或者数据错误\n */\nconst isBlanks: <T>(data:T) => boolean = (data) => {\n    if(data == Infinity) {\n        return !!data;\n    }\n    // 判断Object类型\n    if (typeof data === 'object' && data !== null) {\n        const propsArr = Object.getOwnPropertyNames(data);\n        return !propsArr.length\n    }\n    // 判断数组类型\n    if (Array.isArray(data)) {\n        return !data.length;\n    }\n\n    // 判断空格的字符串和转义符\n    if(typeof data === 'string') {\n        return /^\\s*$/.test(data)\n    }\n\n    // 判断空Set和空Map\n    if (data instanceof Set || data instanceof Map) {\n        return data.size === 0\n    }\n\n    // 判断错误时间\n    if (data instanceof Date) {\n        return Number.isNaN(data.getTime());\n    }\n    return !data;\n}\n\nexport default isBlanks;","/Users/darwish/Desktop/darwish-react-hooks/src/functions/isBrowser.ts",[],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useScroll/index.demo.tsx",["134"],[],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useScroll/index.tsx",[],["135","136","137","138","139","140","141","142","143","144","145","146","147"],"/Users/darwish/Desktop/darwish-react-hooks/src/hooks/useAsync/index.demo.tsx",[],[],{"ruleId":"148","severity":1,"message":"149","line":21,"column":17,"nodeType":"150","messageId":"151","endLine":21,"endColumn":25},{"ruleId":"148","severity":1,"message":"152","line":22,"column":9,"nodeType":"150","messageId":"151","endLine":22,"endColumn":17},{"ruleId":"148","severity":1,"message":"153","line":36,"column":7,"nodeType":"150","messageId":"151","endLine":36,"endColumn":15},{"ruleId":"154","severity":1,"message":"155","line":26,"colum